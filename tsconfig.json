{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "target": "ES2021",
    "outDir": "dist",
  },
  "lib": [
    "ES2021.String"
  ]
}